  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier",
      "commit-msg": "yarn lint:commit"
    }
  },
  "browserslist": [
    "last 2 version",
    "Firefox ESR",
    "> 1%",
    "ie >= 9"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.9.0",
    "yarn": ">=1.3.2"
