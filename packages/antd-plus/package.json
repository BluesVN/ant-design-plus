{
  "name": "@alitajs/antd-plus",
  "version": "1.0.2",
  "description": "ant design plus",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:cjs && yarn build:es",
    "build:cjs": "cross-env NODE_ENV=production tsc --module CommonJS --outDir ./lib",
    "build:es": "cross-env NODE_ENV=production tsc --module ES6 --outDir ./build/es",
    "start": "cross-env NODE_ENV=production tsc -w --module ES6 --outDir ./es"
  },
  "peerDependencies": {
    "antd": "^3.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@alitajs/autils": "^0.1.2",
    "antd": "^3.20.7",
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "cross-env": "^5.2.0"
  },
  "overrideBrowserslist": [
    "last 2 version",
    "Firefox ESR",
    "> 1%",
    "ie >= 9"
  ],
  "publishConfig": {
    "access": "public"
  }
}
