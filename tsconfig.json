{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@alitajs/antd-plus": ["src/index.tsx"],
      "@alitajs/antd-plus/es/*": ["src/*"]
    },
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "noUnusedLocals": true,
    "target": "es6",
    "lib": ["dom", "es2017"],
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "lib", "es"]
}
